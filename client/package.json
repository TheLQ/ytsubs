{
  "name": "vite-project",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "publicdev": "vite build --mode development && vite preview --port 3000 --host 0.0.0.0",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "update": "npm-check-updates -u",
    "format": "prettier --write ../server/src src",
    "lint": "eslint --ext .ts,.vue src"
  },
  "dependencies": {
    "vue": "^3.0.5",
    "vue-router": "^4.0.11",
    "vuex": "^4.0.2"
  },
  "sideEffects": false,
  "devDependencies": {
    "@types/gapi.auth2": "^0.0.55",
    "@types/gapi.client.youtube": "^3.0.2",
    "@typescript-eslint/parser": "^4.30.0",
    "@vitejs/plugin-vue": "^1.6.0",
    "@vue/compiler-sfc": "^3.2.8",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-vue": "^7.17.0",
    "npm-check-updates": "^11.8.5",
    "prettier": "^2.3.2",
    "typescript": "^4.4.2",
    "vite": "^2.5.3",
    "vue-tsc": "^0.3.0"
  },
  "type": "module"
}
