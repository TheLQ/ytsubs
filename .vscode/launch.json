  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "runtimeArgs": [
        "--es-module-specifier-resolution=node",
        "-r",
        "source-map-support/register"
      ],
      "request": "launch",
      "name": "web",
      "outputCapture": "std",
      "skipFiles": ["<node_internals>/**"],
      // "preLaunchTask": "tsc: build - tsconfig.json",
      "program": "${workspaceFolder}/src/server/index.ts",
      "outFiles": ["${workspaceFolder}/**/*.js"]
    }
  ]
}
